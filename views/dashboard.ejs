<script>
    const url = 'http://localhost:3000'
    const accessToken = localStorage.getItem("key")

    const frontAuth =  axios.create({
        baseUrl : url,
        headers: {
            Authorization : `Bearer ${accessToken}`
        }
    })
    const result = frontAuth.post(`${url}/dashboard`).then(rs => {
            console.log(rs)
        }).catch(err => {
            console.log(err)
        })
    // console.log(result)
    
</script>